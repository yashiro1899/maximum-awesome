term "xterm-256color"
attrcolor b ".I"
termcapinfo xterm 'Co#256:AB=\E[48;5;%dm:AF=\E[38;5;%dm'
defbce on

escape ^Jj

defscrollback 30000
startup_message off
vbell off

hardstatus alwayslastline "%{bk}%-w%{gk}[%n %t]%{bk}%+w %=%{wk}%c:%s, %Y-%m-%d"


